{
  "dashboard": {
    "id": null,
    "uid": "agent-orchestration",
    "title": "Agent Orchestration Dashboard",
    "tags": ["agents", "orchestration", "monitoring"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Agent Memory Usage",
        "targets": [
          {
            "expr": "container_memory_usage_bytes{container=~\".*agent.*|.*claude.*|.*gemini.*|.*codeium.*\"}",
            "legendFormat": "{{container}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "bytes",
            "show": true
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Agent CPU Usage",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{container=~\".*agent.*|.*claude.*|.*gemini.*|.*codeium.*\"}[5m]) * 100",
            "legendFormat": "{{container}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "show": true
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Active Agents",
        "targets": [
          {
            "expr": "orchestrator_active_agents",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Task Queue Size",
        "targets": [
          {
            "expr": "orchestrator_queue_size",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value",
          "colorMode": "background",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "piechart",
        "title": "Task Status Distribution",
        "targets": [
          {
            "expr": "sum by (status) (orchestrator_tasks_total)",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "displayLabels": ["name", "percent"],
          "legendDisplayMode": "list",
          "legendPlacement": "right"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Task Execution Duration",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(orchestrator_task_duration_seconds_bucket[5m])) by (le, task_type))",
            "legendFormat": "p95 - {{task_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum(rate(orchestrator_task_duration_seconds_bucket[5m])) by (le, task_type))",
            "legendFormat": "p50 - {{task_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "show": true
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Resource Violations",
        "targets": [
          {
            "expr": "rate(resource_manager_violations[5m])",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "ops",
            "show": true
          },
          {
            "show": false
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24},
        "type": "table",
        "title": "Container Resource Usage",
        "targets": [
          {
            "expr": "topk(10, container_memory_usage_bytes{container=~\".*agent.*|.*claude.*|.*gemini.*|.*codeium.*|orchestrator|resource-manager\"})",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Value"},
              "properties": [
                {
                  "id": "unit",
                  "value": "bytes"
                },
                {
                  "id": "custom.displayMode",
                  "value": "lcd-gauge"
                }
              ]
            }
          ]
        }
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "type": "heatmap",
        "title": "Task Processing Heatmap",
        "targets": [
          {
            "expr": "sum(increase(orchestrator_task_duration_seconds_bucket[1m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": true,
          "calculation": {
            "xBuckets": {
              "mode": "size",
              "value": "1min"
            },
            "yBuckets": {
              "mode": "count",
              "value": "10"
            }
          },
          "color": {
            "mode": "spectrum",
            "scheme": "Oranges"
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "type": "alertlist",
        "title": "Active Alerts",
        "options": {
          "showOptions": "current",
          "maxItems": 10,
          "sortOrder": 1,
          "dashboardAlerts": false,
          "alertName": "",
          "dashboardTitle": "",
          "tags": []
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "agent",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(container_memory_usage_bytes, container)",
          "regex": ".*agent.*|.*claude.*|.*gemini.*|.*codeium.*",
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}